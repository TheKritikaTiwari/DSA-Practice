class Solution 
{ 
    int sameChar(String A, String B) 
    {
        // code here
        A=A.toUpperCase();
        B=B.toUpperCase();
        int c=0;
        for(int i=0;i<A.length();i++){
            char a=A.charAt(i);
            char b=B.charAt(i);
            if(a==b){
                c++;
            }
        }
        return c;
    }
} 